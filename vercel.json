{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "methods": [
        "POST"
      ],
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "methods": [
        "GET",
        "POST",
        "OPTIONS"
      ],
      "dest": "api/index.py"
    }
  ]
}